# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_project_url
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Mapbox Configuration
EXPO_PUBLIC_MAPBOX_ACCESS_TOKEN=your_mapbox_access_token

# Server-side Environment Variables (for Supabase Edge Functions)
# These should be set in your Supabase project settings under Edge Functions > Environment Variables

# Google Maps API Configuration
GOOGLE_MAPS_API_KEY=your_google_maps_api_key
GOOGLE_VISION_API_KEY=your_google_vision_api_key

# Feature Flags
FEATURE_USE_TESSERACT=false
DEFAULT_REGION=US

# Supabase Service Role Key (for Edge Functions)
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key